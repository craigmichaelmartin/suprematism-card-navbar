{
  "name": "suprematism-card-navbar",
  "version": "0.0.0",
  "license": "MIT",
  "main": "src/index.ts",
  "typings": "src/index.d.ts",
  "scripts": {
    "start": "ng serve",
    "lint": "ng lint",
    "pretest": "npm update",
    "test": "npm run prettier-test && npm run lint && npm run test:e2e",
    "test:ci": "npm run prettier-test && npm run lint && npm run test:e2e:ci",
    "test:e2e": "ng e2e",
    "pretest:e2e:ci": "webdriver-manager update --versions.chrome=2.29",
    "test:e2e:ci": "ng e2e --progress false --config protractor.sauce.conf.js --webdriver-update false",
    "_prettier": "prettier --print-width 80 --tab-width 2 --single-quote --trailing-comma none",
    "prettier": "npm run _prettier -- --write  \"src/**/*.ts\"",
    "prettier-test": "npm run _prettier -- -l \"src/**/*.ts\"",
    "commit": "transform-index-and-commit 'npm run _prettier -- --write'",
    "prepush": "assert-on-head 'npm run test'"
  },
  "private": true,
  "peerDependencies": {
    "suprematism-style-guide": "github:cinbcuniversal/suprematism-style-guide"
  },
  "dependencies": {
    "suprematism-dropdown-image": "github:cinbcuniversal/suprematism-dropdown-image"
  },
  "devDependencies": {
    "@angular/cli": "1.3.0",
    "@angular/common": "^4.3.4",
    "@angular/compiler": "^4.3.4",
    "@angular/compiler-cli": "^4.3.4",
    "@angular/core": "^4.3.4",
    "@angular/forms": "^4.3.4",
    "@angular/http": "^4.3.4",
    "@angular/platform-browser": "^4.3.4",
    "@angular/platform-browser-dynamic": "^4.3.4",
    "@angular/router": "^4.3.4",
    "@types/jasmine": "^2.5.53",
    "@types/node": "^8.0.20",
    "codelyzer": "^3.1.2",
    "core-js": "^2.5.0",
    "git-quality": "github:CINBCUniversal/git-quality",
    "jasmine-core": "^2.7.0",
    "jasmine-spec-reporter": "^4.2.0",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-sass": "^4.4.0",
    "prettier": "^1.5.3",
    "protractor": "~5.1.0",
    "rxjs": "^5.1.0",
    "suprematism-style-guide": "github:cinbcuniversal/suprematism-style-guide",
    "ts-helpers": "^1.1.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.6.0",
    "typescript": "^2.4.2",
    "zone.js": "^0.8.16"
  }
}
